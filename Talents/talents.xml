<?xml version="1.0" encoding="utf-8"?>
<Override>
<Talents>

<!-- down with the ship -->

<Talent identifier="downwiththeship">
    <Description tag="talentdescription.downwiththeship">
      <Replace tag="[flooded]" value="50" color="gui.green"/>
      <Replace tag="[swimmingboost]" value="50" color="gui.green"/>
      <Replace tag="[skillboost]" value="25" color="gui.green"/>
      <Replace tag="[duration]" value="360" color="gui.green"/>
    </Description>
    <Icon texture="Content/UI/TalentsIcons1.png" sheetindex="0,1" sheetelementsize="128,128"/>
    <AbilityGroupInterval interval="1">
      <Conditions>
        <AbilityConditionShipFlooded floodpercentage="0.5"/>
      </Conditions>
      <Abilities>
        <CharacterAbilityApplyStatusEffects>
          <StatusEffects>
            <StatusEffect type="OnAbility" target="Character" disabledeltatime="true" multiplyafflictionsbymaxvitality="true">
              <Affliction identifier="downwiththeship" amount="360.0"/>
            </StatusEffect>
          </StatusEffects>
        </CharacterAbilityApplyStatusEffects>
      </Abilities>
    </AbilityGroupInterval>
  </Talent>

<!-- captain drunken sailor -->

 <Talent identifier="drunkensailor">
    <Description tag="talentdescription.drunkensailor2">
      <Replace tag="[dmgresistance]" value="30" color="gui.green"/>
      <Replace tag="[stunresistance]" value="50" color="gui.green"/>
      <Replace tag="[skillgainprobability]" value="4" color="gui.green"/>
    </Description>
    <Icon texture="Content/UI/TalentsIcons1.png" sheetindex="3,0" sheetelementsize="128,128"/>
    <AbilityGroupInterval>
      <Conditions>
        <AbilityConditionHasAffliction afflictionidentifier="drunk"/>
      </Conditions>
      <Abilities>
        <CharacterAbilityModifyResistance resistanceid="damage" resistance="0.7"/>
        <CharacterAbilityModifyResistance resistanceid="stun" resistance="0.5"/>
      </Abilities>
    </AbilityGroupInterval>
	<AbilityGroupEffect abilityeffecttype="None">
      <Abilities>
        <CharacterAbilityGiveResistance resistanceid="nausea" multiplier="0"/>
      </Abilities>
    </AbilityGroupEffect>
    <AbilityGroupEffect abilityeffecttype="OnUseRangedWeapon">
      <Conditions>
        <AbilityConditionItem identifiers="harmonica"/>
        <AbilityConditionServerRandom randomchance="0.04"/>
      </Conditions>
      <Abilities>
        <CharacterAbilityApplyStatusEffectsToRandomAlly allowdifferentsub="false">
          <StatusEffects>
            <StatusEffect type="OnAbility" target="Character">
              <GiveSkill skillidentifier="randomskill" amount="1"/>
            </StatusEffect>
          </StatusEffects>
        </CharacterAbilityApplyStatusEffectsToRandomAlly>
      </Abilities>
    </AbilityGroupEffect>
  </Talent>

  <!-- combat medic talent -->
  
  <Talent identifier="combatmedic">
    <Icon texture="Content/UI/TalentsIcons1.png" sheetindex="7,5" sheetelementsize="128,128"/>
    <Description tag="talentdescription.unlockrecipe">
      <Replace tag="[itemname]" value="entityname.combatstimulantsyringe" color="gui.orange"/>
    </Description>
    <Description tag="talentdescription.combatmedic">
      <Replace tag="[skillpercentage]" value="30" color="gui.green"/>
    </Description>
    <AbilityGroupInterval interval="0.5">
      <Abilities>
        <CharacterAbilityModifyStatToSkill stattype="MovementSpeed" maxvalue="0.3" skillidentifier="medical"/>
        <CharacterAbilityModifyStatToSkill stattype="MeleeAttackSpeed" maxvalue="0.3" skillidentifier="medical"/>
        <CharacterAbilityModifyStatToSkill stattype="SwimmingSpeed" maxvalue="0.3" skillidentifier="medical"/>
      </Abilities>
    </AbilityGroupInterval>
    <AddedRecipe itemidentifier="combatstimulantsyringe"/>
  </Talent>
 
